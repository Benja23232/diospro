{% extends 'base.html.twig' %}

{% block title %}Inscripcion{% endblock %}

{% block body %}
    <h1>Inscripcion</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ inscripcion.id }}</td>
            </tr>
            <tr>
                <th>FechaInscripcion</th>
                <td>{{ inscripcion.fechaInscripcion ? inscripcion.fechaInscripcion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Participante</th>
                <td>{{ inscripcion.participante.nombre }} {{inscripcion.participante.apellido}}</td>
            </tr>
            <tr>
                <th>Evento</th>
                <td>{{ inscripcion.evento.nombre}}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_inscripcion_index') }}">back to list</a>

    <a href="{{ path('app_inscripcion_edit', {'id': inscripcion.id}) }}">edit</a>

    {{ include('inscripcion/_delete_form.html.twig') }}
{% endblock %}
